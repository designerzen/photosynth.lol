  "description": "Royal Society Summer Exhibition 2025 Landing page",
  "source": "source/index.pug",
  "scripts": {
    "zip": "node action-zip-wavetable.js",
    "dev": "parcel ./source/sitemap.html --port 808",
    "start": "npm run zip && npm run dev",
    "index": "parcel ./source/index.html --port 808",
    "midi": "parcel ./source/midi.html --port 808",
    "build": "parcel build ./source/sitemap.html",
    "publish": "npm run zip && npm run build",
    "upgrader": "npx npm-check-updates --interactive -u",
    "test": "echo \"Error: no test specified\" && exit 1",
    "push:": "git push origin --tags && git push origin",
    "patch": "npm run patch && npm run build",
    "minor": "npm run minor && npm run build",
    "major": "npm run major && npm run build",
    "release": "npm run patch && npm run build && npm run push"
  },
  "keywords": [
    "science",
    "exhibition",
    "royal society",
    "summer exhibition",
    "2025"
  ],
  "staticFiles": {
    "staticFiles": [
      {
        "staticPath": "static"
      }
    ]
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "author": "designerzen@gmail.com",
  "license": "ISC",
